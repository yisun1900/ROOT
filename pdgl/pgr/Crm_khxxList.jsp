	{
		if (!(jyjdrq.equals("")))	wheresql+="  and (jyjdrq>=TO_DATE('"+jyjdrq+"','YYYY/MM/DD'))";
	}
	jyjdrq=request.getParameter("jyjdrq2");
	if (jyjdrq!=null)
	{
		if (!(jyjdrq.equals("")))	wheresql+="  and (jyjdrq<=TO_DATE('"+jyjdrq+"','YYYY/MM/DD'))";
